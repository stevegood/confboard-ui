{
  "name": "confboard-ui",
  "version": "1.0.0",
  "description": "A simple UI for keeping track of conference sessions and attaching notes to them.",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "NODE_ENV=test jest",
    "coverage": "NODE_ENV=test jest --coverage",
    "dist": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevegood/confboard-ui.git"
  },
  "engines": {
    "node": "0.10",
    "npm": "2.10"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "production",
    "sass",
    "hot",
    "reload"
  ],
  "author": "Steve Good <sgood@lanctr.com> (http://lanctr.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevegood/confboard-ui/issues"
  },
  "dependencies": {
    "alt": "^0.17.1",
    "compression": "^1.5.2",
    "ejs": "^2.3.3",
    "express": "^4.13.3",
    "express-ejs-layouts": "^1.1.0",
    "material-ui": "^0.11.0",
    "node-uuid": "^1.4.3",
    "object-assign": "^4.0.1",
    "react": "^0.13.3",
    "react-substance": "^0.0.6-Alpha",
    "react-tap-event-plugin": "^0.1.7"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^2.2.2",
    "autoprefixer-loader": "^2.1.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "coveralls": "^2.11.4",
    "css-loader": "^0.17.0",
    "eslint": "^1.3.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.3.1",
    "jest-cli": "^0.5.0",
    "jsx-loader": "^0.13.2",
    "node-libs-browser": "^0.5.2",
    "react-hot-loader": "^1.3.0",
    "react-tools": "^0.13.3",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testFileExtensions": [
      "test.js",
      "test.jsx"
    ],
    "testDirectoryName": "client"
  }
}
